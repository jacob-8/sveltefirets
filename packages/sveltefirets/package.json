{
  "name": "sveltefirets",
  "version": "0.0.32",
  "scripts": {
    "dev": "vite dev --mode kitbook",
    "build": "vite build --mode kitbook",
    "preview": "vite preview --mode kitbook",
    "package": "svelte-kit sync && svelte-package && node update-packaged-index-path.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
    "firebase": "^9.15.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@sveltejs/package": "1.0.1",
    "kitbook": "1.0.0-alpha.3",
    "replace-in-file": "^6.3.5",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.6.0",
    "tslib": "^2.3.1",
    "typescript": "~4.9.4",
    "vite": "^4.0.3"
  },
  "type": "module",
  "svelte": "src/lib/index.ts",
  "main": "src/lib/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-8/sveltefirets.git"
  },
  "keywords": [
    "Svelte",
    "SvelteKit",
    "Firebase",
    "ESM",
    "Typescript"
  ],
  "author": "Jacob Bowdoin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacob-8/sveltefirets/issues"
  },
  "homepage": "https://github.com/jacob-8/sveltefirets#readme"
}