{
  "name": "sveltefirets",
  "version": "0.0.33",
  "description": "Use Firebase in a declarative manner in Svelte",
  "scripts": {
    "dev": "vite dev --mode kitbook",
    "build": "vite build --mode kitbook",
    "preview": "vite preview --mode kitbook",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --threshold warning --diagnostic-sources js,svelte --watch"
  },
  "dependencies": {
    "firebase": "^9.21.0"
  },
  "peerDependencies": {
    "firebase": "^9.21.0",
    "svelte": "^3.58.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.0.1",
    "@sveltejs/kit": "1.15.10",
    "@sveltejs/package": "2.0.2",
    "kitbook": "1.0.0-alpha.32",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-pieces": "^1.0.62",
    "svelte-preprocess": "^5.0.3",
    "svelte2tsx": "^0.6.11",
    "publint": "^0.1.9",
    "tslib": "^2.5.0",
    "typescript": "~4.9.4",
    "vite": "^4.3.4"
  },
  "type": "module",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-8/sveltefirets.git"
  },
  "keywords": [
    "Svelte",
    "SvelteKit",
    "Firebase",
    "ESM",
    "Typescript"
  ],
  "author": "Jacob Bowdoin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacob-8/sveltefirets/issues"
  },
  "homepage": "https://github.com/jacob-8/sveltefirets#readme"
}